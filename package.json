        "axios": "^1.6.8",
        "bindings": "^1.5.0",
        "class-variance-authority": "^0.7.0",
        "cli-progress": "^3.12.0",
        "clsx": "^2.1.0",
        "dompurify": "^3.0.6",
        "electron-squirrel-startup": "^1.0.0",
        "faiss-node": "^0.5.1",
        "flowbite": "^2.2.1",
        "fs-extra": "^10.1.0",
        "jquery": "^3.7.1",
        "katex": "^0.16.10",
        "langchain": "^0.2.8",
        "mammoth": "^1.7.2",
        "markdown-it": "^13.0.2",
        "marked": "^11.0.0",
        "naudiodon2": "^2.4.0",
        "node-aplay": "^1.0.3",
